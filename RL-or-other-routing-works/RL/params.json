{
    "env": {
        "env_name": "vlsi-dat",
        "resolution": 0.5,
        "benchmark_folder": "../../PCBs",
        "pcb_names": ["1Bitsy_1bitsy_v5"],
        "termination_rule": "v",
        "design_rule_violate_penalty_coef": 10
    },
    "rl":{
