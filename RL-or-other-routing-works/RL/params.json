{
    "env": {
        "env_name": "vlsi-dat",
        "resolution": [0.5, 0.5],
        "benchmark_folder": "../../PCBs",
        "pcb_names": ["1Bitsy_1bitsy"],
        "termination_rule": "vr",
        "design_rule_violate_penalty_coef": 10
    },
    "rl":{
        "rl_model_name": "ppo",
        "total_timesteps": 3e3,
        "n_epochs": 10,
        "learning_rate": 0.0001,
        "n_steps": 1000,
        "batch_size": 50,
        "gamma": 0.99,
        "gae_lambda": 0.97,
        "ent_coef": 0.1,
        "vf_coef": 0.5,
        "device": "auto",
        "policy_network": "MlpPolicy",
        "actor_net_structure": [32, 32],
        "critic_net_structure": [32, 32],
        "activation_function": "Relu",
        "save_model_name": "test",
        "load_model_name": null
    }
}