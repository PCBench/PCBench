        "batch_size": 64,
        "gamma": 0.99,
        "gae_lambda": 0.95,
        "ent_coef": 0.1,
        "vf_coef": 0.5,
        "device": "auto",
        "policy_network": "MlpPolicy",
        "actor_net_structure": [64, 64],
        "critic_net_structure": [64, 64],
        "activation_function": "Relu",
        "save_model_name": null,
        "load_model_name": null
    }
